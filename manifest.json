{
    "name": "Urban Dictionary Search",
    "version": "0.0",
    "description": "Search Urban Dictionary definitions anywhere on the web!",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "content_security_policy": "script-src 'self' https://unpkg.com; object-src 'self'",
    "manifest_version": 2
  }
