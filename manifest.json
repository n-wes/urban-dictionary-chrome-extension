{
    "name": "Urban Dictionary Search",
    "version": "0.0",
    "description": "Search Urban Dictionary definitions anywhere on the web!",
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "manifest_version": 2
  }
